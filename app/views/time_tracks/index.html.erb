<h1>Listing time_tracks</h1>

<table>
  <tr>
    <th>Start</th>
    <th>End</th>
    <th>Interrumptions</th>
    <th>Client</th>
    <th>Project</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_tracks.each do |time_track| %>
  <tr>
    <td><%= time_track.start_time %></td>
    <td><%= time_track.end_time %></td>
    <td><%= time_track.interrumptions %></td>
    <td><%= time_track.client %></td>
    <td><%= time_track.project %></td>
    <td><%= time_track.notes %></td>
    <td><%= link_to 'Show', time_track %></td>
    <td><%= link_to 'Edit', edit_time_track_path(time_track) %></td>
    <td><%= link_to 'Destroy', time_track, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time track', new_time_track_path %>
